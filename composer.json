        "Exception",
        "Emarketa"
    ],
    "authors": [
        {
            "name": "Andy Myers (Emarketa)",
            "email": "andymyers@emarketa.com"
        }
    ],
    "require": {
        "magento/framework": "^100.1|^101.0|^102.0",
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Emarketa\\AdvancedLogger\\": "src/AdvancedLogger"
        },
        "files": [
            "src/AdvancedLogger/registration.php"
        ]
    }
